public class marks {  
public static void main(String[] args) {  
    int marks=50;  
      
    if(marks>60){  
        System.out.println("SUCCESSFUL");  
    }  
    else if(marks>=89){  
        System.out.println("Grade A");  
    }
     else if(marks>=70){  
        System.out.println("Grade B");  
    }  
     else if(marks>=50){  
        System.out.println("Grade D");  
    }  
    else{  
        System.out.println("UNSUCCESSFUL");  
    }  
}  
}  
